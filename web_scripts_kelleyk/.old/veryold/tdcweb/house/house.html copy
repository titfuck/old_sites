bName = navigator.appName;
bVer = parseInt(navigator.appVersion);
	if	((bName == "Netscape" && bVer >= 3) ||
		(bName == "Microsoft Internet Explorer" && bVer >= 4)) br = "n3";
	else
		br = "n2";




if (br == "n3") {

	img1on = new Image();
	img1on.src = "fourth_glow.jpg";
	img2on = new Image();
	img2on.src = "third_glow.jpg";
	img3on = new Image();
	img3on.src = "second_glow.jpg";
	img4on = new Image();
	img4on.src = "first_glow.jpg";
	img5on = new Image();
	img5on.src = "base_glow.jpg";
	
	img1off = new Image();
	img1off.src = "fourth_norm.jpg";
	img2off = new Image();
