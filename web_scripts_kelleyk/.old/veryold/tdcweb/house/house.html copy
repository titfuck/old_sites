	img4off = new Image();
	img4off.src = "first_norm.jpg";
	img5off = new Image();
	img5off.src = "base_norm.jpg";


}

function changePages(floorURL) {
	if (br == "n3") {
		parent.cutaway.location.href = floorURL;
		parent.room.location.href = "rooms/room_intro.html";
	}
}

function imgAct(imgName) {
	if (br == "n3") {
		document[imgName].src = eval(imgName + "on.src");
	}
}

function imgInact(imgName) {
	if (br == "n3") {
		document[imgName].src = eval(imgName + "off.src");
	}
}
// Stop hiding from old browsers -->
